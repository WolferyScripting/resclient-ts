{
  "name": "resclient-ts",
  "version": "1.1.3",
  "description": "A RES client written in typescript.",
  "packageManager": "pnpm@9.6.0",
  "types": "./dist/esm/index.d.mts",
  "module": "./dist/esm/index.mjs",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "sh build.sh",
    "prepack": "npm run build",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "test": "c8 mocha",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WolferyScripting/resclient-ts.git"
  },
  "author": "Donovan_DMC <hewwo@yiff.rocks>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WolferyScripting/resclient-ts/issues"
  },
  "homepage": "https://github.com/WolferyScripting/resclient-ts#readme",
  "devDependencies": {
    "@swc/core": "^1.3.69",
    "@swc/helpers": "^0.5.1",
    "@types/chai": "^5.2.2",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.4.2",
    "@types/sinon": "^17.0.4",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "c8": "^10.1.3",
    "chai": "^5.2.1",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "debug": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsdoc": "^50.6.11",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "sinon": "^21.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "tslib": "^2.8.1",
    "ws": "^8.13.0"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "provenance": true
  }
}
